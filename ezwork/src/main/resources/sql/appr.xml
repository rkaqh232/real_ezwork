<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 영민 결재  -->

<mapper namespace="Apprs">
	<select id="getlist" parameterType="java.util.Map" resultType="appr">
		select * from
		(select rownum rnum, b.*
		from
		(select * from
		approve
		order by APPR_CODE desc) b
		)
		where rnum
		&gt;=#{start} and rnum &lt;= #{end}
	</select>
	
	<select id="count" resultType="int">
		select count(*) from approve
	</select>
	
	<insert id="insert" parameterType="appr">
		insert into approve
		value
	</insert>
	
	<select id="getname" parameterType="string">
		select M_NAME
		from member
		where M_NAME=
	</select>
	
	<select id="getpart" parameterType="string">
		select M_PART_C
		from member
		where M_PART_C=
	</select>
	
</mapper>